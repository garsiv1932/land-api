dotnet ef migrations add JWTImplementation -o Migrations -c ApiContext --project ../Domain/Domain.csproj --verbose


dotnet ef database update 20210722113420_JWTImplementation -c ApiContext -p ../Domain/Domain.csproj -s Api.csproj --no-build --verbose


dotnet ef migrations remove --project ../Domain/Domain.csproj

dotnet ef migrations list


dotnet ef migrations script -o script.sql


